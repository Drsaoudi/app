        <% include ../partials/header %>
    </header>

    <div class="container">

        <!-- Navbar goes here -->

        <!-- Page Layout here -->
        <div class="row">
            <div id="main" class="nine columns">
              <button id="big-button" class="button-primary">CREATE YOUR ORDER</button>

            </div>

            <div id="lists" class="three columns">
              <% for(var i = 0; i < coffee.length; i++){ %>
